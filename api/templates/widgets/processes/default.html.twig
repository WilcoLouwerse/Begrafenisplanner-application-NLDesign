<input type="hidden" name="submitters[0][brp]" value="{{ user['@id'] }}">
<input type="hidden" name="requestType" value="{{ process.requestType }}">
<input type="hidden" name="status" value="incomplete">
<input type="hidden" name="organization" value="{{ commonground_resource(process.requestType)['organization'] }}">
<input type="hidden" name="processType" value="{{ process['@id'] }}">
{% if stage.next is defined and stage.next is not null and stage.next != false %}
    <input type="hidden" name="currentStage" value="{{ stage.next['@id'] }}">
    {% else %}
    <input type="hidden" name="next" value="indienen">
{% endif %}
{% if request is defined and request is not null %}
    <input type="hidden" name="@id" value="{{ request['@id'] }}">
{#    {{ dump(request) }}#}
{% endif %}
